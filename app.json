{
  "expo": {
    "name": "dotExchange",
    "slug": "dotcoin",
    "version": "2.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00ADA200"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.renanmakoto.dotexchange",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.renanmakoto.dotexchange",
      "versionCode": 17,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "968a9733-cf5a-4ddd-987b-692ff2e13925"
      }
    },
    "owner": "renanmakoto"
  },
  "cli": {
    "appVersionSource": "local"
  }
}
